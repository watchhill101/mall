.OrderS {
  padding: 16px;
}

.OrderS .header {
  margin-bottom: 16px;
}

.OrderS .header .btn-list {
  display: flex;
  gap: 10px;
  list-style: none;
  padding: 0;
}

.OrderS .header .btn-list a {
  padding: 6px 12px;
  border: 1px solid #ccc;
  border-radius: 4px;
  text-decoration: none;
  color: #333;
}

.OrderS .header .btn-list a.active {
  background-color: #1677ff;
  color: #fff;
  border-color: #1677ff;
}

.OrderS .search-bar {
  margin-bottom: 16px;
}

.OrderS .table-section {
  background-color: #fff;
  padding: 16px;
  border-radius: 8px;
}
